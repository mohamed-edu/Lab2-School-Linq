@model IEnumerable<Lab2_School_Linq.Models.Student>

@{
    ViewData["Title"] = "The Students";
}

<h1>All Students</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                Students
            </th>
            <th>
                Class
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.StudentName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CourseClass.CourseClassName)
                </td>
            </tr>
        }
    </tbody>
</table>
